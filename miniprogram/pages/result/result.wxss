/* pages/result/result.wxss */

page{
  background: #fff;
    background: #fff;
  --red: #c04851;
	--orange: rgb(209, 97, 82);
	--blue: #5e7987;
}


.titleContainer{
  position: relative;
  box-sizing: border-box;
  padding: 100rpx;
  height: 150rpx;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: space-evenly;
  align-items: center;
  height: 80rpx;
  padding-left: 50rpx;
  padding-right: 50rpx;
}
.td {
    width:50%;
    text-align: center;
}
.th {
  width: 30%;
  height: 40rpx;
  background: #5e7987;
  color: #fff;
  text-align: center;
  border-radius: 5rpx;
}

.th2 {
  width: 30%;
  height: 40rpx;
  background: #fff;
  color:black;
  text-align: center;
  border-radius: 5rpx;
  font-weight: 500;
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

.UCenter-bg {
  background-image: url(data:image/png;base64,);
  background-size: cover;
  height: 400rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
  z-index: 1;
}

.zindex{
  z-index: 2;
  color: black;
  font-weight: 700;
}

.confinewidth2{
  width:150rpx;
  border-radius: 10rpx;
}
